--- /dev/null
+++ b/arch/arm/dts/rk3566-ts-pi-u-boot.dtsi
@@ -0,0 +1,18 @@
+#include "rk356x-u-boot.dtsi"
+
+/ {
+	chosen {
+		stdout-path = &uart2;
+	};
+};
+
